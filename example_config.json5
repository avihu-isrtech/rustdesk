// You can use this file as a reference to create your own config file, as comments are allowed here and each field is explained either by its name or a comment

{
  "conn-type": "incoming", // incoming/outgoing
  "disable-account": "Y",
  "disable-ab": "Y",
  "disable-chat-screen": "Y",
  "default-settings": {
    "direct-server": "N",
    "custom-rendezvous-server": "192.168.7.48",
    "key": "O7HxZReFltfqa2minq4ktbxmyCfFW4kyH5r1APTZAt8=" // the public key of the rendezvous server
  },
  "override-settings": {
    "peer-id": "555555555", // a predefined peer ID - can be an empty string, null or removed if we want to auto generate
    "auto-peer-id-mode": "based-on-uuid", // numeric - only numbers / uuid - completely random string / based-on-uuid - calculate it based on the uuid of the device (default is numeric)
    "fixed-uuid": "some_random_uuid_value", // this is the unique identifier of the device - to let the app pick its own device id leave it null, empty or remove it
    "floating-window-untouchable": "Y",
    "floating-window-size": "32", // in pixels (its square shape)
    "floating-window-transparency": "1", // 0-10 (0 - invisible, 10 - fully visible)
    "floating-window-initial-x": "0",
    "floating-window-initial-y": "0",
    "hide-app-bar-actions-in-server-page": "Y",
    "disallow-turning-off-given-permissions": "Y",
    "hide-qr-code-scanner-in-settings-page": "Y",
    "show-start-on-boot-option": "server_page", // server_page/settings_page
    "hide-server-settings": "Y",
    "hide-2fa-settings": "Y",
    "verification-method": "use-permanent-password", // use-temporary-password/use-permanent-password/use-both-passwords
    "approve-mode": "password" // password/click/both
  }
}
